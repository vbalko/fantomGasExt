{
    "manifest_version": 3,
  
    "name": "Fantom Gas Tracker",
    "description": "Gas tracker on fantom network",
    "version": "0.1.0",
  
    "action": {
     "default_title": "Fantom Gas Tracker",
     "default_icon": "icons/icon_256.png",
     "default_popup": "popup.html"
    },
    "background": {
        "service_worker": "background.js"
    },
    "permissions": ["storage"]
  }